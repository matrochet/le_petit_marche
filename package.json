{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/copy-images.mjs && next dev --turbopack",
    "build": "node scripts/copy-images.mjs && next build --turbopack",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "seed": "tsx prisma/seed.ts",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "assets:copy": "node scripts/copy-images.mjs",
    "audit:responsive": "node src/utils/test-responsiveness.js --dir src --format console",
    "audit:responsive:json": "node src/utils/test-responsiveness.js --dir src --format json --out reports/responsiveness.json",
    "audit:responsive:md": "node src/utils/test-responsiveness.js --dir src --format md --out reports/responsiveness.md"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.1",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stripe": "^18.5.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.1",
    "stripe-cli": "^0.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
